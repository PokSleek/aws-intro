{
  "name": "aws-intro",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "serverless deploy",
    "deploy-prod": "serverless deploy --stage prod",
    "invoke-create": "serverless invoke local -f createConsent --path mocks/createConsent.json --watch",
    "invoke-prop-insert": "serverless invoke local -f propagateConsent --path mocks/propagateConsent__insert.json --watch",
    "invoke-prop-modify": "serverless invoke local -f propagateConsent --path mocks/propagateConsent__modify.json --watch",
    "invoke-publish": "serverless invoke local -f publishConsent --path mocks/publishConsent.json --watch"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "serverless": "^1.60.5",
    "serverless-pseudo-parameters": "^2.5.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "aws-sdk": "^2.596.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "serverless-webpack": "^5.2.0",
    "source-map-support": "^0.5.10",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
